                            $('.fetchspechideshow').show();
                            $('.fetch_specification_'+bdid).hide();
                            
                            

                                                
                        }
                        if (hasSaved == false && roleid != 5)
                        {
                            $('#Step1End').html('<input data-bidDetail="' + bdid + '" type="button" value="Save" class="saveSpecs btn btn-md btn-primary" style="float:right">');
                        }
                        

                    } else
                    {
                        if(roleid == 5)
                        {
                            var hasSavedtech = $('.hiddens_'+bdid+' .techSpecs_show').hasClass('techspecactive');
                            if(hasSavedtech == true)
                            {
                                $('.hiddens_'+bdid+' .techSpecs_show').removeClass('techspecactive');
                                $('.hiddens_'+bdid+' .techSpecs_show').html('');       
                                $('.hiddens_'+bdid).hide();
                            }
                            else
                            {
                                $('.hiddens_'+bdid+' .techSpecs_show').addClass('techspecactive');
                                $('.hiddens_'+bdid+' .techSpecs_show').html('There is some issue while fetching technical specifications please try again !');
                                $('.hiddens_'+bdid).show();
                            }
                        }
                        else
                        {
                                                    $('.hidespecfication_'+bdid+' .specfication_show').html('There is some issue while fetching technical specifications please try again !');  
                            $('.hidespecfication_'+bdid+' .specfication_show').show();
                            //$('.hidespecfication_'+bdid).show();
                            //$('.specfication_step2').show();
                           // $('.specfication_step2 .specfication_show').html('There is some issue while fetching technical specifications please try again !');
                                                 }
                    }

                }
            });
        });

    });</script>
