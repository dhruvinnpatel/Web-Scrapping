<script>
    function checkLoadData() {
        var loadbased_required = $('input[name="loadbased_required"]:checked').val();

        if(loadbased_required == undefined || loadbased_required == 'undefined')
        {
            $('.loadbased_error').html('This is required');
            return false;
        }

        if((loadbased_required != undefined) && (loadbased_required == 1))
        {
            $('.red').html('');
            var loadbased_reason = $(".loadbased_reason").val().trim();
            if(loadbased_reason == '') {
                $('.loadbased_reason_error').html('This is required');
                return false;
            }
            if(loadbased_reason.length < 50 || loadbased_reason.length > 5000) {
                $('.loadbased_reason_error').html('Reason must be between 50 to 5000 characters');
                return false;
            }

            var loadbased_doc_required = $('input[name="loadbased_doc_required"]:checked').val();

            if(loadbased_doc_required == undefined || loadbased_doc_required == 'undefined')
            {
                $('.loadbased_doc_error').html('This is required');
                return false;
            }

            if((loadbased_doc_required != undefined) && (loadbased_doc_required == 1))
            {
                var loadbased_file = $("#loadbased_file").val();
                if(loadbased_file == '' && $("#prev_loadbased_file").val() == '') {
                    $('.loadbased_file_error').html('This is required');
                    return false;
                }
            }

        } 
        return true;
    }

    $(document).on('change', '#loadbased_file', function (e) {
        const file = document.getElementById($(this).attr('id')); 
        $('.loadbased_file_error').html('');
        if(file.files.length > 0) {
            const fSize = file.files.item(0).size;
            const fName = file.files.item(0).name;
            const fExt = fName.split('.')[fName.split('.').length - 1].toLowerCase();
            
            if(fSize > 0 && fSize <= 5242880) {
                if(fExt != 'pdf') {
                    alert("Please upload document in PDF file only");
                    $(this).val('')
                }
            } else {
                alert("Maximum supported file size for upload is 5 MB");
                $(this).val('')
            }
        } else {
            alert("No file selected");
            $(this).val('')
        }
    });

